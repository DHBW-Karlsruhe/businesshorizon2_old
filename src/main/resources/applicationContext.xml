<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd    
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
    <bean
        class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor" />
    <bean
        class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor" />



    <!-- Application -->
    <bean name="vaadinApp" class="dhbw.ka.mwi.businesshorizon2.BHApplication"
        scope="session" />
    <bean name="eventBus" class="com.mvplite.event.EventBus" scope="session" />
    <bean name="navigationController" class="com.mvplite.view.LiteNavigationController"
        scope="session">
        <constructor-arg ref="eventBus" />
        <!--not break-->
    </bean>

    <!-- Services -->
    <bean name="authenticationService" 
   		class="dhbw.ka.mwi.businesshorizon2.services.authentication.AuthenticationService" 
    	scope="singleton"/>
    <bean name="persistenceService" 
   		class="dhbw.ka.mwi.businesshorizon2.services.persistence.PersistenceService" 
    	scope="singleton"/>
    
    <!-- Methods -->
    <bean name="timeseries"
        class="dhbw.ka.mwi.businesshorizon2.methods.timeseries.TimeseriesCalculator"
        scope="session" />
    <bean name="wiener" class="dhbw.ka.mwi.businesshorizon2.methods.wiener.Wiener"
        scope="session" />
    <bean name="random" class="dhbw.ka.mwi.businesshorizon2.methods.random.RandomWalk"
		scope="session" />


    <!-- Models -->
    <bean name="project" class="dhbw.ka.mwi.businesshorizon2.models.Project"
        scope="session" />
    <bean name="user" class="dhbw.ka.mwi.businesshorizon2.models.User"
        scope="session" />
    
    <!-- Services -->
    <bean name="projectProxy" class="dhbw.ka.mwi.businesshorizon2.services.proxies.ProjectProxy"
        scope="session" />

    <bean name="userProxy" class="dhbw.ka.mwi.businesshorizon2.services.proxies.UserProxy"
        scope="session" />
    

    <!-- LogIn -->
    <bean name="logInPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.login.LogInScreenPresenter"
        scope="session" />
    <bean name="logInScreenView" class="dhbw.ka.mwi.businesshorizon2.ui.login.LogInScreenViewImpl"
        scope="session" />

    <!-- Initial-Screen -->
    <bean name="initialScreenPresenter"
        class="dhbw.ka.mwi.businesshorizon2.ui.initialscreen.InitialScreenPresenter"
        scope="session" />
    <bean name="initialScreenView"
        class="dhbw.ka.mwi.businesshorizon2.ui.initialscreen.InitialScreenViewImpl"
        scope="session" />

    <!-- Project-List -->
    <bean name="projectListPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.initialscreen.projectlist.ProjectListPresenter" scope="session" />
    <bean name="projectListView"      class="dhbw.ka.mwi.businesshorizon2.ui.initialscreen.projectlist.ProjectListViewImpl"  scope="session" />

    <!-- Infos -->
    <bean name="infosViewPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.initialscreen.infos.InfosPresenter" scope="session" />
    <bean name="infosView"          class="dhbw.ka.mwi.businesshorizon2.ui.initialscreen.infos.InfosViewImpl"  scope="session" />
        
    <!-- Process -->
    <bean name="processPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.ProcessPresenter" scope="session" />
    <bean name="processView"      class="dhbw.ka.mwi.businesshorizon2.ui.process.ProcessViewImpl"  scope="session" />

	<!-- Process-Navigation -->
	<bean name="navigationPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.navigation.NavigationPresenter" scope="session" />
	<bean name="navigationView"      class="dhbw.ka.mwi.businesshorizon2.ui.process.navigation.NavigationViewImpl"  scope="session"/>


    <!-- ContentContainer -->
    <bean name="contentContainerPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.contentcontainer.ContentContainerPresenter" scope="session" />
    <bean name="contentContainerView"      class="dhbw.ka.mwi.businesshorizon2.ui.process.contentcontainer.ContentContainerViewImpl"  scope="session"/>
    

	<!-- Methodenmaske -->
	<bean name="methodPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.method.MethodPresenter" scope="session" />
	<bean name="methodView"      class="dhbw.ka.mwi.businesshorizon2.ui.process.method.MethodViewImpl"  scope="session" />
	
	
	
	<!-- Periodenmaske -->
	<bean name="periodPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.PeriodPresenter" scope="session" />
	<bean name="periodView"      class="dhbw.ka.mwi.businesshorizon2.ui.process.period.PeriodViewImpl"  scope="session" />
	
	<!-- Periode-Timeline -->
	<bean name="timelinePresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.timeline.TimelinePresenter" scope="session"/>
	<bean name="timelineView" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.timeline.TimelineViewImpl" scope="session"/>
	
	<!-- Periode-DirektEingabe -->
	<bean name="direktPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.input.direkt.DirektPresenter" scope="session"/>
	<bean name="direktView" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.input.direkt.DirektViewImpl" scope="session"/>
	
	
	<!-- Periode-GesamtEingabe -->
	<bean name="gesamtPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.input.gesamt.GesamtPresenter" scope="session"/>
	<bean name="gesamtView" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.input.gesamt.GesamtViewImpl" scope="session"/>
	
	
	
	<!-- Periode-UmsatzEingabe -->
	<bean name="umsatzPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.input.umsatz.UmsatzPresenter" scope="session"/>
	<bean name="umsatzView" class="dhbw.ka.mwi.businesshorizon2.ui.process.period.input.umsatz.UmsatzViewImpl" scope="session"/>
	
	
	<!-- Parameter-Maske -->
	<bean name="parameterPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.parameter.ParameterPresenter" scope="session" />
	<bean name="parameterView"      class="dhbw.ka.mwi.businesshorizon2.ui.process.parameter.ParameterViewImpl"  scope="session" />
	

    
    <!-- Szenarien-Maske -->
    <bean name="scenarioPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.scenario.ScenarioPresenter" scope="session" />
    <bean name="scenarioView"      class="dhbw.ka.mwi.businesshorizon2.ui.process.scenario.ScenarioViewImpl"  scope="session" />
    
    
    
    <!-- Ausgabe-Maske -->
    <bean name="outputPresenter" class="dhbw.ka.mwi.businesshorizon2.ui.process.output.OutputPresenter" scope="session" />
    <bean name="outputView"      class="dhbw.ka.mwi.businesshorizon2.ui.process.output.OutputViewImpl"  scope="session" />
</beans>


